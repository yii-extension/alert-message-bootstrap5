{
    "name": "yii-extension/alert-message-bootstrap5",
    "type": "library",
    "description": "Alert Message Bootstrap5 Widget",
    "keywords": [
        "yii3",
        "widget",
        "alert-message",
        "bulma"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "yiisoft/session": "^1.0",
        "yiisoft/yii-bootstrap5": "dev-clean-up-rectoring as 3.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.3.0",
        "oomphinc/composer-installers-extender": "^2.0.0",
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.7",
        "vimeo/psalm": "^4.4",
        "yiisoft/di": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\Widget\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\Widget\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "installer-types": [
            "npm-asset"
        ],
        "installer-paths": {
            "./node_modules/{$name}": [
                "type:npm-asset"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/terabytesoftw/yii-bootstrap5"
        }
    ]
}
