{
    "name": "yii-extension/alert-flash-bootstrap5",
    "type": "library",
    "description": "Alert Flash Bootstrap5 Widget",
    "keywords": [
        "yii3",
        "alert-flash",
        "bootstrap5",
        "widget"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "yii-extension/bootstrap5": "^1.0.0",
        "yiisoft/session": "^1.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.4.0",
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.8",
        "vimeo/psalm": "^4.8",
        "yiisoft/di": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\Bootstrap5\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\Bootstrap5\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
